<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <RuntimeIdentifier>browser-wasm</RuntimeIdentifier>
        <BootsharpName>backend</BootsharpName>
        <BootsharpPackageDirectory>$(MSBuildThisFileDirectory)</BootsharpPackageDirectory>
        <BootsharpAggressiveTrimming>true</BootsharpAggressiveTrimming>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Bootsharp" Version="0.1.0-rc.158"/>
        <PackageReference Include="Naninovel.Common.Bindings" Version="2023.10.16.1901"/>
        <PackageReference Include="Naninovel.Language.Bindings" Version="2023.10.13.305"/>
    </ItemGroup>

</Project>
